{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "${fileDirname}/Tile.cpp",
                "${fileDirname}/LinkedList.cpp",
                "${fileDirname}/Node.cpp",
                "${fileDirname}/LinkedListTest.cpp",
                "${fileDirname}/Game.cpp",
                "${fileDirname}/Bag.cpp",
                "${fileDirname}/BoxLid.cpp",
                "${fileDirname}/Factory.cpp",
                "${fileDirname}/Player.cpp",
                "${fileDirname}/StorageRows.cpp",
                "${fileDirname}/Wall.cpp",
                "${fileDirname}/BrokenTiles.cpp",
                "${fileDirname}/mosaicTesting.cpp",
                "${fileDirname}/TestTile.cpp",
                "${fileDirname}/TestLoadGame.cpp"
            ],
            "options": {
                "cwd": "/usr/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
